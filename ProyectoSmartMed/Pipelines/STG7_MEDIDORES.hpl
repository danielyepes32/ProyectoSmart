    <connection>POSTGRES_SMART</connection>
    <execute_each_row>N</execute_each_row>
    <limit>0</limit>
    <sql>SELECT 
    h.*,
    ft.idtiempo AS create_time_id,
    fts.id_ts AS create_ts_id
FROM 
    smart_med.stg6_medidores h
JOIN 
    smart_med.final_tiempo_medidores ft 
    ON ft.year = CAST(SUBSTRING(h.create_date::varchar FROM 1 FOR 4) AS INTEGER)
    AND ft.month = CAST(SUBSTRING(h.create_date::varchar FROM 6 FOR 2) AS INTEGER)
    AND ft.day = CAST(SUBSTRING(h.create_date::varchar FROM 9 FOR 2) AS INTEGER)
JOIN 
    smart_med.final_timestamp_medidores fts 
    ON fts.hour = CAST(SUBSTRING(h.create_date::varchar FROM 12 FOR 2) AS INTEGER)
    AND fts.minute = CAST(SUBSTRING(h.create_date::varchar FROM 15 FOR 2) AS INTEGER)
    AND fts.second = CAST(SUBSTRING(h.create_date::varchar FROM 18 FOR 2) AS INTEGER);</sql>
