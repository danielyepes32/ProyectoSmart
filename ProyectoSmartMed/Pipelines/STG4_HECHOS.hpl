    <limit>0</limit>
    <sql>WITH ranked_hechos AS (
    SELECT
        *,
        ROW_NUMBER() OVER (
            PARTITION BY recv_time_id, meter_id, (recv_ts_id / 10800) -- Agrupa por d√≠a, medidor y intervalos de 3 horas
            ORDER BY 
                CASE 
                    WHEN gateway_id = 'WalkBy' THEN 1 
                    ELSE 0 
                END, -- Prioriza los registros sin WalkBy
                recv_ts_id -- Ordena por timestamp en caso de empate
        ) AS row_num
    FROM smart_med.stg3_hechos
)
SELECT *
FROM ranked_hechos
WHERE row_num = 1;
</sql>
