    <sql>-- Registros que cumplen con la condición
SELECT
    gateway_id,
    service_center,
    project,
    coordenadas,
    -- Extraer latitud y longitud usando expresiones regulares
    regexp_matches(coordenadas, '(-?\d+\.\d+)(-?\d+\.\d+)') AS coords,
    (regexp_matches(coordenadas, '(-?\d+\.\d+)(-?\d+\.\d+)'))[1] AS latitud,
    (regexp_matches(coordenadas, '(-?\d+\.\d+)(-?\d+\.\d+)'))[2] AS longitud,
    comment,
    address,
    freq,
    status,
    district
FROM
    smart_med.stg2_gateways
WHERE
    coordenadas ~ '(-?\d+\.\d+)(-?\d+\.\d+)'

UNION ALL

-- Registros que no cumplen con la condición
SELECT
    gateway_id,
    service_center,
    project,
    coordenadas,
    NULL AS coords,
    latitude AS latitud,
    longitude AS longitud,
    comment,
    address,
    freq,
    status,
    district
FROM
    smart_med.stg2_gateways
WHERE
    coordenadas !~ '(-?\d+\.\d+)(-?\d+\.\d+)'
ORDER BY gateway_id;
</sql>
