    <sql>WITH numbered_alarmas AS (
    SELECT
        alarm_id,
        alarm_time,
        meter_code,
        recv_time,
        fallo_type,
        falla_desc,
        ROW_NUMBER() OVER (PARTITION BY alarm_id ORDER BY alarm_time) - 1 AS alarm_index,
        CAST(alarm_id AS TEXT) || '' || CAST(ROW_NUMBER() OVER (PARTITION BY alarm_id ORDER BY alarm_time) - 1 AS TEXT) AS combined_id_desc
    FROM smart_med.stg3_alarmas
)
SELECT
    alarm_id,
    alarm_time,
    meter_code,
    recv_time,
    fallo_type,
    falla_desc,
    alarm_index,
    combined_id_desc
FROM numbered_alarmas
ORDER BY alarm_id, alarm_index;
</sql>
