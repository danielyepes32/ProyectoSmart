    <sql>WITH times AS (
    SELECT generate_series(0, 86399) AS seconds_in_day
)
SELECT 
    seconds_in_day / 3600 AS hora,
    (seconds_in_day % 3600) / 60 AS minuto,
    seconds_in_day % 60 AS segundo,
    (seconds_in_day / 3600) * 10000 + ((seconds_in_day % 3600) / 60) * 100 + (seconds_in_day % 60) AS id
FROM 
    times;
</sql>
