    <connection>POSTGRES_SMART</connection>
    <execute_each_row>N</execute_each_row>
    <limit>0</limit>
    <sql>SELECT 
    h.*,
    ft.idtiempo AS recv_time_id,
    fts.id_ts AS recv_ts_id,
    rtrim(h.meter_id)
FROM 
    smart_med.stg2_hechos h
JOIN 
    smart_med.final_tiempo_hechos ft 
    ON ft.year = CAST(SUBSTRING(h.recv_time FROM 1 FOR 4) AS INTEGER)
    AND ft.month = CAST(SUBSTRING(h.recv_time FROM 6 FOR 2) AS INTEGER)
    AND ft.day = CAST(SUBSTRING(h.recv_time FROM 9 FOR 2) AS INTEGER)
JOIN 
    smart_med.final_timestamp_hechos fts 
    ON fts.hour = CAST(SUBSTRING(h.recv_time FROM 12 FOR 2) AS INTEGER)
    AND fts.minute = CAST(SUBSTRING(h.recv_time FROM 15 FOR 2) AS INTEGER)
    AND fts.second = CAST(SUBSTRING(h.recv_time FROM 18 FOR 2) AS INTEGER);
</sql>
